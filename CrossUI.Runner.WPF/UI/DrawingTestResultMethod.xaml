<UserControl x:Class="CrossUI.Runner.WPF.UI.DrawingTestResultMethod"
			 xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
			 xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
			 xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
			 xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
			 mc:Ignorable="d" 
			 d:DesignHeight="300" d:DesignWidth="300" Margin="0,8,0,0">
	<Grid>
		<Grid.ColumnDefinitions>
			<ColumnDefinition Width="Auto"/>
			<ColumnDefinition Width="*"/>
		</Grid.ColumnDefinitions>

		<Border Grid.Column="0" BorderBrush="LightGray" BorderThickness="2" Padding="2">
			<ContentPresenter Name="Result" />
		</Border>
		<Grid Grid.Column="1" VerticalAlignment="Bottom">
			<Label Margin="8,0,0,0" VerticalAlignment="Bottom" Name="MethodName" >MethodName</Label>
			<Label HorizontalAlignment="Right" Margin="16,0,0,0" FontSize="11" Visibility="Collapsed" Name="Report"></Label>
		</Grid>
	</Grid>
</UserControl>
